@model MRT_Demo.Models.Indicator

@{
    Dictionary<int, string> PeriodDic = ViewBag.PeriodDic;
}
@Html.HiddenFor(m => m.ForeCastPeriodSelected_Index, new { Value = 0 })
<div class="mb-3">
    @foreach (KeyValuePair<int, string> i in PeriodDic)
    {
        <input value="@i.Value" type="submit" onclick="ChangeMonthQuarterHailfYear(@Html.IdFor(m => m.ForeCastPeriodSelected_Index),@i.Key)" />
    }
</div>
@Html.EditorFor(m => m.ImportantIndicatorResultMeasurement, new { QuarterIndex = Model.PeriodSelected_Index })
