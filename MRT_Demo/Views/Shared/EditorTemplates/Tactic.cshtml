@model MRT_Demo.Models.Tactic

@Html.HiddenFor(m => m.IsDeleteTactic, new { Value = false })
@Html.HiddenFor(m => m.IsDelete)
@Html.HiddenFor(m => m.IsLastDelete)
@Html.HiddenFor(m => m.CreateDate)
@Html.HiddenFor(m => m.UpdateDate)
@Html.HiddenFor(m => m.ID)
@Html.HiddenFor(m => m.StrategyID)
@Html.HiddenFor(m => m.NO)
@if (Model.IsDelete == false)
{

    <tr>
        <td>
            @Html.EditorFor(m => m.Tactic1)
        </td>
        <td>
            <input type="submit" value="ลบ" onclick="DeleteTactic(@Html.IdFor(m => m.IsDeleteTactic))" />
        </td>
    </tr>
}
else
{
    @Html.HiddenFor(m => m.Tactic1);
}
